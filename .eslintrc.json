{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "airbnb-base"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "no-console": [
      "error"
    ],
    "quotes": [
      2,
      "single"
    ],
    "import/prefer-default-export": [
      "off"
    ]
  }
}